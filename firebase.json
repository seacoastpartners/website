{
    "hosting": {
        "public": "dist",
        "rewrites": [
            {
                "source": "/book-meeting",
                "destination": "/book-meeting.html"
            },
            {
                "source": "/mvp",
                "destination": "/mvp.html"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ]
    }
}